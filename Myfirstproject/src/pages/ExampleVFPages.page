<apex:page standardController="Account" recordSetVar="accounts" tabstyle="account" sidebar="false">
  <apex:pageBlock >
    <apex:pageBlockTable value="{!accounts}" var="a">
      <apex:column value="{!a.name}"/>
      <apex:column value="{!a.type}"/>
    </apex:pageBlockTable>
  </apex:pageBlock>
</apex:page>